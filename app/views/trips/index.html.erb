<h1>Listing trips</h1>

<table>
  <tr>
    <th>Original eta</th>
    <th>Phone guid</th>
    <th>Latest eta</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @trips.each do |trip| %>
  <tr>
    <td><%= trip.original_eta %></td>
    <td><%= trip.phone_guid %></td>
    <td><%= trip.latest_eta %></td>
    <td><%= link_to 'Show', trip %></td>
    <td><%= link_to 'Edit', edit_trip_path(trip) %></td>
    <td><%= link_to 'Destroy', trip, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

